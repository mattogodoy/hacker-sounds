{
    "name": "hacker-sounds",
    "publisher": "mattogodoy",
    "displayName": "Hacker Sounds",
    "description": "This extension automatically turns you into a very skilled hacker by playing movie-like sounds while you write code.",
    "version": "1.4.3",
    "engines": {
        "vscode": "^1.40.0"
    },
    "homepage": "https://matto.io",
    "repository": {
        "type": "git",
        "url": "https://github.com/mattogodoy/hacker-sounds"
    },
    "keywords": [
        "sound",
        "keyboard",
        "hacker",
        "fx",
        "sci-fi"
    ],
    "icon": "icon.png",
    "license": "MIT",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "hacker_sounds.enable",
                "title": "Hacker Sounds: Enable"
            },
            {
                "command": "hacker_sounds.disable",
                "title": "Hacker Sounds: Disable"
            },
            {
                "command": "hacker_sounds.volumeUp",
                "title": "Hacker Sounds: Volume Up"
            },
            {
                "command": "hacker_sounds.volumeDown",
                "title": "Hacker Sounds: Volume Down"
            },
            {
                "command": "hacker_sounds.setVolume",
                "title": "Hacker Sounds: Set Volume",
                "args": "newVol"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run -S esbuild-base -- --minify",
        "esbuild-base": "esbuild ./src/extension.js --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
        "esbuild": "npm run -S esbuild-base -- --sourcemap",
        "esbuild-watch": "npm run -S esbuild-base -- --sourcemap --watch",
        "test-compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/lodash.debounce": "^4.0.6",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.40.0",
        "esbuild": "^0.14.23",
        "glob": "^7.1.5",
        "lodash": "^4.17.21",
        "mocha": "^10.2.0",
        "tslint": "^5.20.0",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "lodash": "^4.17.21",
        "lodash.debounce": "^4.0.8",
        "play-sound": "^1.1.3"
    }
}